    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "slick-carousel": "^1.8.1"
  },
  "scripts": {
    "build-betaaa": "npm run clean-dist && ./node_modules/.bin/cross-env NODE_OPTIONS='--max-old-space-size=4096' webpack --config configs/webpack/prod.js",
    "clean-dist": "rimraf build/*",
    "start-prod": "webpack  serve --node-env production --config webpack.config.js  --profile --content-base dist/ ",
    "dev": "webpack serve --node-env development  --config webpack.config.js  --profile --content-base src/ ",
    "start": "npm-run-all --parallel build start-prod",
    "storybook": "start-storybook -p 6006 -s public",
    "serve": "webpack serve --config webpack.config.js --profile --watch --content-base src/",
    "build": "npm run clean-dist && cross-env NODE_ENV=production webpack",
    "lint": "eslint  src/**/*.{js,jsx,ts,tsx} --format stylish --project . --force",
    "lint:fix": "eslint src/**/*.{js,jsx,ts,tsx} --fix",
    "netlify:deploy": "netlify deploy --dir=./dist -p -m \"$(git log -1 --pretty=%B)\"",
    "test": "NODE_ENV=test && jest  --coverage  --watchAll --config=configs/jest.json",
    "test:watch": "NODE_ENV=test && jest --watch --coverage --config=configs/jest.json",
    "build-storybook": "build-storybook -s public",
    "analyze": "cross-env NODE_ENV=production ANALYZE=1 webpack",
    "deploy": "cross-env GZIP=1 DEPLOY_S3=1 npm run build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.13.1",
    "@babel/plugin-transform-runtime": "^7.13.15",
    "@babel/preset-env": "^7.13.0",
    "@babel/preset-react": "^7.12.13",
    "@babel/preset-typescript": "^7.13.0",
    "@types/history": "^4.7.8",
    "@types/node": "^14.14.43",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.1",
    "@types/react-redux": "^7.1.16",
    "@types/react-router-dom": "^5.1.7",
    "@typescript-eslint/eslint-plugin": "^4.15.2",
    "@typescript-eslint/parser": "^4.15.2",
    "@webpack-cli/serve": "^1.3.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "cross-env": "^7.0.3",
    "css-loader": "^5.0.2",
    "dotenv-webpack": "^7.0.2",
    "eslint": "^7.20.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "file-loader": "^6.2.0",
    "html-loader": "^2.1.1",
    "html-webpack-plugin": "^4.2.0",
    "jest": "^26.6.3",
    "mini-css-extract-plugin": "^1.4.1",
    "node-sass": "^5.0.0",
    "prettier": "^2.0.5",
    "sass-loader": "^11.0.1",
    "style-loader": "^1.3.0",
    "webpack": "^5.24.0",
    "webpack-cli": "^4.5.0",
    "webpack-dashboard": "^3.3.1",
    "webpack-dev-server": "^3.11.2"
  },
  "proxy": "http://localhost:5000"
}
