    "generate-password": "^1.6.0",
    "install": "^0.13.0",
    "npm-run-all": "^4.1.5",
    "raven-js": "^3.27.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-final-form": "^6.5.3",
    "react-infinite-scroll-hook": "^4.0.1",
    "react-paypal-button-v2": "^2.6.3",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0",
    "react-share": "^4.4.0",
