{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "axios": "^0.21.0",
    "csstype": "^2.6.15",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-final-form": "^6.5.3",
    "react-infinite-scroll-hook": "^4.0.1",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0"
  },
  "scripts": {
    "build-devvvv": "npm run clean-dist && webpack --config=configs/webpack/prod.js --env API_URL=https://api-geekcamp.herokuapp.com/api/v1",
    "build-betaaa": "npm run clean-dist && ./node_modules/.bin/cross-env NODE_OPTIONS='--max-old-space-size=4096' webpack --config configs/webpack/prod.js --env API_URL=https://api.vortic.io/",
    "clean-dist": "rimraf dist/*",
    "start": "npm run start:dev",
    "start-devvvv": "webpack serve --mode development --config=configs/webpack/dev.js --env API_URL=https://api-geekcamp.herokuapp.com/api/v1 --port 3000 --hot --host 0.0.0.0",
    "start:dev": "webpack serve  --env API_URL=https://api-geekcamp.herokuapp.com/api/v1 PORT=3000 NODE_ENV=development --config webpack.config.js  --progress --profile --content-base src/ --hot",
    "start-prodddd": "npm-run-all --parallel build server",
    "storybook": "start-storybook -p 6006 -s public",
    "serve": "webpack serve --config webpack.config.js --progress --profile --watch --content-base src/",
    "build": "npm run clean-dist && cross-env NODE_ENV=production webpack",
    "lint": "eslint  src/**/*.{js,jsx,ts,tsx} --format stylish --project . --force",
    "lint:fix": "eslint src/**/*.{js,jsx,ts,tsx} --fix",
    "netlify:deploy": "netlify deploy --dir=./dist -p -m \"$(git log -1 --pretty=%B)\"",
    "test": "NODE_ENV=test && jest  --coverage  --watchAll --config=configs/jest.json",
    "test:watch": "NODE_ENV=test && jest --watch --coverage --config=configs/jest.json",
    "build-storybook": "build-storybook -s public"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.13.1",
    "@babel/plugin-transform-runtime": "^7.13.15",
    "@babel/preset-env": "^7.13.0",
    "@babel/preset-react": "^7.12.13",
    "@babel/preset-typescript": "^7.13.0",
    "@storybook/react": "^6.1.19",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/history": "^4.7.8",
    "@types/node": "^14.14.43",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.1",
    "@types/react-redux": "^7.1.16",
    "@types/react-router-dom": "^5.1.7",
    "@typescript-eslint/eslint-plugin": "^4.15.2",
    "@typescript-eslint/parser": "^4.15.2",
    "@webpack-cli/serve": "^1.3.0",
    "awesome-typescript-loader": "^5.2.1",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "babel-polyfill": "^6.26.0",
    "cross-env": "^7.0.3",
    "css-loader": "^5.0.2",
    "dotenv-webpack": "^7.0.2",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "eslint": "^7.20.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "file-loader": "^6.2.0",
    "html-loader": "^2.1.1",
    "html-webpack-plugin": "^4.2.0",
    "image-webpack-loader": "^7.0.1",
    "jest": "^26.6.3",
    "mini-css-extract-plugin": "^1.4.1",
    "netlify-cli": "^3.8.5",
    "node-sass": "^5.0.0",
    "patch-package": "^6.4.7",
    "postcss-import": "^14.0.1",
    "postcss-loader": "^3.0.0",
    "prettier": "^2.0.5",
    "sass-loader": "^11.0.1",
    "style-loader": "^1.3.0",
    "tslint": "^6.1.3",
    "tslint-webpack-plugin": "^2.1.0",
    "typescript": "^4.1.5",
    "webpack": "^5.24.0",
    "webpack-cli": "^4.5.0",
